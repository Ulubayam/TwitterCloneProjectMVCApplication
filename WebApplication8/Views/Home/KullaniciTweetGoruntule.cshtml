@model IEnumerable<WebApplication8.Models.Tweet>

@{
    ViewBag.Title = "KullaniciTweetGoruntule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">
       
        <li>@Html.ActionLink("Güncelle", "Update", "Home")</li>
      


    </ul>
</div>

<div class="container">
    <div class="panel panel-default">
       

        @foreach (var item in Model)
        {
            <div class="panel-heading" style="margin-top:10px;">@Html.DisplayFor(ModelItem => item.kullaniciNick)</div>
            <div class="panel-body">@Html.DisplayFor(modelItem => item.tweetText)</div>
            <div class="panel-footer">@Html.DisplayFor(modelItem => item.favoriSayisi) |@Html.ActionLink("Favorilere Ekle", "FavorilereEkle", new { tweetId = item.tweetID })|@Html.ActionLink("Tweet Sil", "TweetSil", new { tweetId = item.tweetID })</div>


                   
              
        }

    </div>
</div>
