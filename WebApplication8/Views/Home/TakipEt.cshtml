@model IEnumerable<WebApplication8.Models.KullaniciTakip>

@{
    ViewBag.Title = "Takip Edilenler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.kullaniciNick)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.kullaniciNick)
        </td>
        <td>
            @Html.ActionLink("Takip Bırak", "Delete", new { /* id=item.PrimaryKey */ })
          
        </td>
    </tr>
}

</table>
